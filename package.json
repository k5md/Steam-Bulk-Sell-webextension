{
  "name": "Steam-Bulk-Sell-webextension",
  "scripts": {
    "build-dev": "npx rimraf ./dist && npx webpack --config ./webpack.dev.js",
    "build-prod": "npx rimraf ./dist && npx webpack --config ./webpack.prod.js",
    "browser": "web-ext run --config .web-ext-config.js",
    "pack": "npm run build-prod && web-ext build --config .web-ext-config.js",
    "watch": "npm run build-dev && concurrently \"npx webpack --config ./webpack.dev.js --watch\" \"web-ext run --config .web-ext-config.js\""
  },
  "author": "k5md",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-jsx": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cache-loader": "^4.1.0",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.1",
    "rimraf": "^2.7.1",
    "sass-loader": "^8.0.0",
    "speed-measure-webpack-plugin": "^1.3.1",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.3",
    "vhtml": "^2.2.0",
    "web-ext": "^3.2.0",
    "webextension-polyfill": "^0.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "mithril": "^2.0.4"
  }
}
