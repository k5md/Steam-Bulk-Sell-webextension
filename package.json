    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "bootstrap": "4.4.1",
    "cache-loader": "4.1.0",
    "classnames": "2.2.6",
    "concurrently": "4.1.2",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.19.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.8.0",
    "lodash": "4.17.15",
    "mini-css-extract-plugin": "0.8.2",
    "mobx": "5.15.4",
